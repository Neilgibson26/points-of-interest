<form action="/dashboard" method="POST">
  <div class="d-flex justify-content-between p-5">
    <div class="row align-items-center" style="width: 70%">
      <button class="btn btn-outline-info" type="button">Add Placemark</button>
    </div>
    <div class="d-flex">
      <select
        class="form-control border border-info"
        style="width: 200px;"
        name="category"
      >
        <option>{{category}}</option>
        {{#each choices}}
          <option>{{choice}}</option>
        {{/each}}

      </select>
      <button type="submit" class="button is-info mx-2">Filter</button>
    </div>

  </div>

</form>